{
  "expo": {
    "name": "TiCO Scan",
    "slug": "tico",
    "version": "2.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tico",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "bundleIdentifier": "com.tico.foodhea.tico",
      "associatedDomains": [
        "applinks:tico.foodhea.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "versionCode": 8,
      "package": "com.tico.foodhea.tico",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": {
            "scheme": "https",
            "host": "tico.foodhea.com",
            "pathPrefix": "/"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "com.google.android.gms.permission.AD_ID",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "imageWidth": 150
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The $(PRODUCT_NAME) accesses your photos to let you share them."
        }
      ],
      "expo-font",
      "expo-notifications",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "ticoapp",
    "runtimeVersion": "7.1.0",
    "updates": {
      "url": "https://u.expo.dev/c784d721-95d0-447c-bcd5-1b86d9e7f0fb"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d1c1dc9e-8f75-4c4f-ac45-488e14c84320"
      }
    }
  }
}
